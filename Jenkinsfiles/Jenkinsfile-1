@Library('devopscube-demo-library')_

pipeline {
    agent any
    environment {
        deployEnv = 'dev'
    }
    stages {

        stage("Cleanup Workspace"){
            steps {
                deleteDir()
            }
        }

        stage('Git Checkout'){
            steps {
                Git('https://github.com/spring-projects/spring-petclinic.git', 'master')
            }
        }
    }
}